<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="de.tmaximini.HotelReview" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>HotelReview</name>
  <description>&#xD;
        Sample Hotel Review App&#xD;
    </description>
  <author email="kyawzawwin@gmail.com" href="https://sg.linkedin.com/in/kyawzawwin1">&#xD;
      Kyaw Zaw Win&#xD;
    </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" version="1" />
  <access origin="*"/>
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>

  <platform name="android">
    <icon src="res/android/ldpi.png" density="ldpi" />
    <icon src="res/android/mdpi.png" density="mdpi" />
    <icon src="res/android/hdpi.png" density="hdpi" />
    <icon src="res/android/xhdpi.png" density="xhdpi" />
  </platform>

  <platform name="android">
    <icon src="resources/icons/android/ldpi.png" density="ldpi" />
    <icon src="resources/icons/android/mdpi.png" density="mdpi" />
    <icon src="resources/icons/android/hdpi.png" density="hdpi" />
    <icon src="resources/icons/android/xhdpi.png" density="xhdpi" />
    <icon src="resources/icons/android/xxhdpi.png" density="xxhdpi" />
    <icon src="resources/icons/android/xxxhdpi.png" density="xxxhdpi" />

    <splash src="resources/screens/android/hdpi-landscape.png" density="land-hdpi"/>
    <splash src="resources/screens/android/ldpi-landscape.png" density="land-ldpi"/>
    <splash src="resources/screens/android/mdpi-landscape.png" density="land-mdpi"/>
    <splash src="resources/screens/android/xhdpi-landscape.png" density="land-xhdpi"/>
    <splash src="resources/screens/android/xxhdpi-landscape.png" density="land-xxhdpi"/>
    <splash src="resources/screens/android/xxxhdpi-landscape.png" density="land-xxxhdpi"/>

    <splash src="resources/screens/android/hdpi-portrait.png" density="port-hdpi"/>
    <splash src="resources/screens/android/ldpi-portrait.png" density="port-ldpi"/>
    <splash src="resources/screens/android/mdpi-portrait.png" density="port-mdpi"/>
    <splash src="resources/screens/android/xhdpi-portrait.png" density="port-xhdpi"/>
    <splash src="resources/screens/android/xxhdpi-portrait.png" density="port-xxhdpi"/>
    <splash src="resources/screens/android/xxxhdpi-portrait.png" density="port-xxxhdpi"/>
  </platform>

  <platform name="ios">
    <!-- iOS 8.0+ -->
    <!-- iPhone 6 Plus  -->
    <icon src="resources/icons/ios/144x144.png" width="180" height="180" />
    <!-- iOS 7.0+ -->
    <!-- iPhone / iPod Touch  -->
    <icon src="resources/icons/ios/64x64.png" width="60" height="60" />
    <icon src="resources/icons/ios/114x114.png" width="120" height="120" />
    <!-- iPad -->
    <icon src="resources/icons/ios/72x72.png" width="76" height="76" />
    <icon src="resources/icons/ios/144x144.png" width="152" height="152" />
    <!-- iOS 6.1 -->
    <!-- Spotlight Icon -->
    <icon src="resources/icons/ios/48x48.png" width="40" height="40" />
    <icon src="resources/icons/ios/72x72.png" width="80" height="80" />
    <!-- iPhone / iPod Touch -->
    <icon src="resources/icons/ios/57x57.png" width="57" height="57" />
    <icon src="resources/icons/ios/114x114.png" width="114" height="114" />
    <!-- iPad -->
    <icon src="resources/icons/ios/72x72.png" width="72" height="72" />
    <icon src="resources/icons/ios/144x144.png" width="144" height="144" />
    <!-- iPhone Spotlight and Settings Icon -->
    <icon src="resources/icons/ios/29x29.png" width="29" height="29" />
    <icon src="resources/icons/ios/58x58.png" width="58" height="58" />
    <!-- iPad Spotlight and Settings Icon -->
    <icon src="resources/icons/ios/48x48.png" width="50" height="50" />
    <icon src="resources/icons/ios/114x114.png" width="100" height="100" />

    <splash src="resources/screens/ios/iphone@1x-portrait.png" width="320" height="480"/>
    <splash src="resources/screens/ios/iphone@2x-portrait.png" width="640" height="960"/>
    <splash src="resources/screens/ios/tablet@1x-portrait.png" width="768" height="1024"/>
    <splash src="resources/screens/ios/tablet@2x-portrait.png" width="1536" height="2048"/>
    <splash src="resources/screens/ios/tablet@1x-landscape.png" width="1024" height="768"/>
    <splash src="resources/screens/ios/tablet@2x-landscape.png" width="2048" height="1536"/>
    <splash src="resources/screens/ios/iphone5@2x-portrait.png" width="640" height="1136"/>
    <splash src="resources/screens/ios/iphone6@2x-portrait.png" width="750" height="1334"/>
    <splash src="resources/screens/ios/iphone6@3x-portrait.png" width="1242" height="2208"/>
    <splash src="resources/screens/ios/iphone6@3x-landscape.png" width="2208" height="1242"/>
  </platform>
</widget>